{{>blog/header}}
{{#post}}
    <h1>{{title}}</h1>
    {{#subtitle}}
        <h2>{{subtitle}}</h2>
    {{/subtitle}}
    <h3>Posted By {{creatorInfo.displayName}}</h3>
    <div class="row">
        <div class="col-md-9">
            <div class="post-content">
                {{{content}}}
            </div>
            {{>blog/post-tags}}
        </div>
        <div class="col-md-3">
            {{>blog/search-widget}}
        </div>
    </div>
{{/post}}
{{>blog/footer}}
